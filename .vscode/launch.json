{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Python Debugger: Current File",
            "type": "debugpy",
            "request": "launch",
            "program": "${file}",
            "console": "integratedTerminal",
            "cwd": "${fileDirname}",
            "args": ["-p", "./subfamilies_classification/",
                     "-csv", "./data/positive_database.csv",
                     "-o", "./subfamilies_classification_global",
                    ]
        }
    ]
}
