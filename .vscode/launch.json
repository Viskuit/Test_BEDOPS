{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Python Debugger: Current File",
            "type": "debugpy",
            "request": "launch",
            "program": "${file}",
            "console": "integratedTerminal",
            "cwd": "${fileDirname}",
            "args": [
                     "-csv", "./negative_noCDS_relationship/negative_noCDS_relationship.csv",
                     "-csv2", "./negative_noCDS_relationship/negative_database_nomatch.csv",
                     "-o", "./negative_noCDS_relationship"
                    ]
        }
    ]
}
