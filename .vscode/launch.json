{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Python Debugger: Current File",
            "type": "debugpy",
            "request": "launch",
            "program": "${file}",
            "console": "integratedTerminal",
            "cwd": "${fileDirname}",
            "args": [
                     "-f", "./old_data/fragm_data_marked.json",
                     "-o", "./new_data/",
                     "-db", "./new_data/blastn_dict/TriTrypDB-67_LinfantumJPCM5_Genome.fasta",
                     "-ndb", "./old_data/neg_data_corrected.csv"
                    ]
        }
    ]
}
