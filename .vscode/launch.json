{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Python Debugger: Current File",
            "type": "debugpy",
            "request": "launch",
            "program": "${file}",
            "console": "integratedTerminal",
            "cwd": "${fileDirname}",
            "args": [
                     "-csv", "./data/negative_database_nomatch.csv",
                     "-o", "./"
                    ]
        }
    ]
}
